<section
  style="background-image: url({{ section.settings.banner_img | img_url: 'master' }})"
  class="edy-banner-6 variation-02 bg_image bg_learn_hero header-transperent-spacer"
  data-black-overlay="2"
>
  <div class="wrapper w-100">
    <div>
      <img
        class="banner-logo"
        src="{{ block.settings.logo | img_url: 'master' }}"
        alt="Banner Logo"
      >
    </div>
    <div class="container">
      <div class="row row--15">
        <div class="col-lg-12">
          <div class="banner-mt">
            <div class="row">
              <!-- Left: Banner Text + Button -->
              <div class="col-lg-6 col-md-12 text-left">
                <h2 class="title-landing2 mb-24">{{ section.settings.banner_heading }}</h2>
                <p class="para-landing2-subtitle mb-24 d-none d-sm-block">
                  {{ section.settings.banner_desktop_content }}
                </p>
                <p class="para-home-subtitle d-block d-sm-none">{{ section.settings.banner_mobile_content }}</p>
                <div class="contact-btn xmt--10 text-center mb-24">
                  <a
                    id="btn-quiz"
                    class="edy-btn radius-round replace-url button--full-width"
                    href="{{ section.settings.banner_btn_url }}"
                  >
                    <span class="btn-text" style="font-size: 18px;">{{ section.settings.banner_btn_label }}</span>
                  </a>
                </div>
                <div class="fsa-icons-wrapper mb-24 text-center">
                  <span class="fsa-icon-item">
                    <img
                      src="data:image/png;base64,"
                      alt="icon"
                    >
                    &nbsp;<span>FDA registered</span>
                  </span>
                  <span class="fsa-icon-item">
                    <img
                      src="data:image/png;base64,"
                      alt="icon"
                    >
                    &nbsp;<span>Safe to Use</span>
                  </span>
                </div>
              </div>

              <!-- Right: Testimonial Text -->
              <div class="col-lg-6 col-md-12 text-left position-relative">
                <div class="banner-card-styled">
                  <div class="banner-card-icon-wrapper mb-8">
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 576 512" width="15" height="17" fill="#FFB200">
                      <path d="M259.3 17.8L194 150.2 47.9 171.5c-26.2 3.8-36.7 36.1-17.7 54.6l105.7 103-25 145.5c-4.5 26.3 23.2 46 46.4 33.7L288 439.6l130.7 68.7c23.2 12.2 50.9-7.4 46.4-33.7l-25-145.5 105.7-103c19-18.5 8.5-50.8-17.7-54.6L382 150.2 316.7 17.8c-11.7-23.6-45.6-23.9-57.4 0z"/>
                    </svg>
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 576 512" width="15" height="17" fill="#FFB200">
                      <path d="M259.3 17.8L194 150.2 47.9 171.5c-26.2 3.8-36.7 36.1-17.7 54.6l105.7 103-25 145.5c-4.5 26.3 23.2 46 46.4 33.7L288 439.6l130.7 68.7c23.2 12.2 50.9-7.4 46.4-33.7l-25-145.5 105.7-103c19-18.5 8.5-50.8-17.7-54.6L382 150.2 316.7 17.8c-11.7-23.6-45.6-23.9-57.4 0z"/>
                    </svg>
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 576 512" width="15" height="17" fill="#FFB200">
                      <path d="M259.3 17.8L194 150.2 47.9 171.5c-26.2 3.8-36.7 36.1-17.7 54.6l105.7 103-25 145.5c-4.5 26.3 23.2 46 46.4 33.7L288 439.6l130.7 68.7c23.2 12.2 50.9-7.4 46.4-33.7l-25-145.5 105.7-103c19-18.5 8.5-50.8-17.7-54.6L382 150.2 316.7 17.8c-11.7-23.6-45.6-23.9-57.4 0z"/>
                    </svg>
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 576 512" width="15" height="17" fill="#FFB200">
                      <path d="M259.3 17.8L194 150.2 47.9 171.5c-26.2 3.8-36.7 36.1-17.7 54.6l105.7 103-25 145.5c-4.5 26.3 23.2 46 46.4 33.7L288 439.6l130.7 68.7c23.2 12.2 50.9-7.4 46.4-33.7l-25-145.5 105.7-103c19-18.5 8.5-50.8-17.7-54.6L382 150.2 316.7 17.8c-11.7-23.6-45.6-23.9-57.4 0z"/>
                    </svg>
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 576 512" width="15" height="17" fill="#FFB200">
                      <path d="M259.3 17.8L194 150.2 47.9 171.5c-26.2 3.8-36.7 36.1-17.7 54.6l105.7 103-25 145.5c-4.5 26.3 23.2 46 46.4 33.7L288 439.6l130.7 68.7c23.2 12.2 50.9-7.4 46.4-33.7l-25-145.5 105.7-103c19-18.5 8.5-50.8-17.7-54.6L382 150.2 316.7 17.8c-11.7-23.6-45.6-23.9-57.4 0z"/>
                    </svg>
                  </div>
                  <p class="banner-card-title text-white">
                    {{ section.settings.banner_card_title }}
                  </p>
                  <p class="banner-card-para text-white mb-8">
                    {{ section.settings.banner_card_para }}
                  </p>

                  <div class="banner-card-img-wrapper">
                    <img
                      class="banner-card-avatar"
                      src="{{ block.settings.banner_card_img | img_url: 'master' }}"
                      alt="Testimonial Image"
                    >
                    <span class="banner-card-name-short text-white">{{ section.settings.banner_card_name_short }}</span>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

{% schema %}
{
  "name": "Landing2 Why Eddie",
  "settings": [
    {
      "type": "header",
      "content": "Banner Content"
    },
    {
      "type": "image_picker",
      "id": "logo",
      "label": "Logo"
    },
    {
      "type": "image_picker",
      "id": "banner_img",
      "label": "Banner Image"
    },
    {
      "type": "text",
      "id": "banner_heading",
      "label": "Banner Heading",
      "default": "Traditional Rings Cut Off Blood Flow. Eddie Sets It Free."
    },
    {
      "type": "textarea",
      "id": "banner_desktop_content",
      "label": "Banner Desktop Content",
      "default": "It’s a fact: your penis isn’t shaped like a circle. It’s shaped like an oval. Eddie is designed to work with the natural physiology of an erection. That means harder, longer-lasting, more satisfying erections."
    },
    {
      "type": "textarea",
      "id": "banner_mobile_content",
      "label": "Banner Mobile Content",
      "default": "It’s a fact: your penis isn’t shaped like a circle. It’s shaped like an oval. Eddie is designed to work with the natural physiology of an erection. That means harder, longer-lasting, more satisfying erections."
    },
    {
      "type": "text",
      "id": "banner_btn_label",
      "label": "Banner Button Label",
      "default": "Try Risk Free"
    },
    {
      "type": "url",
      "id": "banner_btn_url",
      "label": "Banner Button URL"
    },
    {
      "type": "text",
      "id": "banner_card_title",
      "label": "Banner Card Title",
      "default": "Can't Believe It Works So Well"
    },
    {
      "type": "textarea",
      "id": "banner_card_para",
      "label": "Banner Card Description",
      "default": "Long-time user of ED Medication. Now I just use Eddie. Without being too explicit, the finales are so much more like the old days from 20+ years ago."
    },
    {
      "type": "image_picker",
      "id": "banner_card_img",
      "label": "Banner Card Image"
    },
    {
      "type": "text",
      "id": "banner_card_name_short",
      "label": "Banner Card Title",
      "default": "R.D."
    }
  ],
  "presets": [
    {
      "name": "Landing2 Why Eddie",
      "category": "Landing2"
    }
  ]
}
{% endschema %}
